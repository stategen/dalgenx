<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <!-- 些内容复制到  /opt/config/stategen/stategen.xml中 -->

    <!-- 应用中启动的端口，供sentinel服务器向应用调用 为啥这个端口不设为 -1 -->
    <entry key="${appName?uncap_first}.client.for.sentinel.transport.port">8721</entry>
    <!-- springboot单独启动时，需要端口，默认8080 -->
    <entry key="${appName?uncap_first}.port">8080</entry>

    <!-- [projectName]_url 对应相应 document.ejs 中的设置，会被导出到index.html,输出时，
       由服务端把值下面相应设置的值传到index.html中,而不应该由前端开发自行写入硬编码，否则开发到发布可能因人工疏忽导致坑 -->
    <entry key="${appName?uncap_first}_url">http://localhost:8080/${appName?uncap_first}</entry>
    <entry key="${appName?uncap_first}.dubbo.qos.port">22222</entry>
    <entry key="${appName?uncap_first}.dubbo.qos.enable">false</entry>
</properties>