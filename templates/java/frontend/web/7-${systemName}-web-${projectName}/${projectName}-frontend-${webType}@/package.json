{
  "private": true,
  "name": "${projectName?uncap_first}",
  "version": "5.0.0-beta",
  "scripts": {
    "dev": "cross-env MOCK=NONE umi dev",
    "build": "umi build",
    "analyze": "cross-env ANALYZE=1 BABELRC=1 umi build",
    "lint": "eslint --fix --ext .js src",
    "lint:js": "eslint --ext .js src",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "test": "cross-env BABELRC=none umi test",
    "prettier": "prettier --write 'src/**/*.(js|less)'",
    "precommit": "lint-staged",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract",
    "trans": "lingui extract --clean && node ./scripts/translate.js",
    "doc": "docsify serve docs"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "classnames": "^2.2.5",
    "d3-shape": "^1.2.0",
    "draftjs-to-html": "^0.8.4",
    "draftjs-to-markdown": "^0.5.1",
    "dva-model-extend": "^0.1.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.1",
    "qs": "^6.5.2",
    "query-string": "^5.1.1",
    "rc-tween-one": "^1.8.1",
    "react-adsense": "^0.0.5",
    "react-countup": "^3.0.3",
    "react-dom": "^16.4.2",
    "react-draft-wysiwyg": "^1.12.13",
    "react-helmet": "^5.2.0",
    "react-highcharts": "^16.0.2",
    "recharts": "^1.0.0-beta.10"
  },
